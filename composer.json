{
  "name": "oxidio/module-seo",
  "type": "oxideshop-module",
  "description": "seo module for oxid shop",
  "license": "GPL-3.0",
  "keywords": [
    "oxid",
    "seo",
    "sitemap",
    "module",
    "google analytics",
    "ga",
    "google tag manager",
    "gtm",
    "enhanced commerce"
  ],
  "require": {
    "oxidio/oxidio": "dev-master",

    "ext-json": "*",
    "php": ">=7.1"
  },
  "require-dev": {
    "oxid-esales/oxideshop-demodata-ce": "^6",
    "oxidio/project": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Oxidio\\": ["src/Oxidio"]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "extra": {
    "oxideshop": {
      "whitelist-filter": [
        "views/**/*",
        "metadata.php"
      ],
      "target-directory": "oxidio/seo"
    }
  },
  "bin": [
    "bin/oxidio-seo"
  ]
}
