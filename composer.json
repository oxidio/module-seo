{
  "name": "oxidio/module-seo",
  "type": "oxideshop-module",
  "description": "seo module for oxid shop",
  "license": "GPL-3.0",
  "homepage": "https://github.com/oxidio/module-seo",
  "authors": [{"name":  "oxidio"}],
  "keywords": [
    "oxid",
    "seo",
    "sitemap",
    "module",
    "google analytics",
    "ga",
    "google tag manager",
    "gtm",
    "enhanced commerce"
  ],
  "require": {
    "oxidio/oxidio": "^1",

    "ext-json": "*",
    "php": ">=7.1"
  },
  "config": {
    "platform": {"php": "7.1"}
  },
  "require-dev": {
    "oxid-esales/oxideshop-demodata-ce": "^6",
    "oxidio/project": "^1"
  },
  "autoload": {
    "psr-4": {
      "Oxidio\\": ["src/Oxidio"]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "extra": {
    "oxideshop": {
      "whitelist-filter": [
        "metadata.php"
      ],
      "target-directory": "oxidio/module-seo"
    },
    "di": "config/seo.php"
  },
  "bin": [
    "bin/oxidio-seo"
  ]
}
