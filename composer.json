{
  "name": "oxidio/module-seo",
  "type": "oxideshop-module",
  "description": "seo module for oxid shop",
  "license": "GPL-3.0",
  "homepage": "https://github.com/oxidio/module-seo",
  "authors": [
    {
      "name": "oxidio"
    }
  ],
  "keywords": [
    "oxid",
    "seo",
    "sitemap",
    "module",
    "google analytics",
    "ga",
    "google tag manager",
    "gtm",
    "enhanced commerce"
  ],
  "require": {
    "oxidio/oxidio": "^6.3",
    "ext-json": "*"
  },
  "require-dev": {
    "oxidio/test": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Oxidio\\": [
        "src/Oxidio"
      ]
    },
    "files": [
      "src/autoload-files.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Oxidio\\": [
        "tests/Oxidio"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "oxideshop": {
      "whitelist-filter": [
        "metadata.php"
      ],
      "target-directory": "oxidio/module-seo"
    },
    "di": "config/seo.php"
  },
  "bin": [
    "bin/oxidio-seo"
  ]
}
